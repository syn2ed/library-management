<?xml version="1.0"?><!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd"><hibernate-mapping>
  <class name="org.formation.gestionbiblio.model.business.User" table="user">
    <id type="int" column="id">
      <generator class="native"/>
    </id>
    <property name="username" type="string" not-null="true" />
    <property name="password" type="string" not-null="true" />
    <property name="role" type="int" not-null="true" />
    <property name="validate" type="boolean" not-null="true" />
  </class>
</hibernate-mapping>